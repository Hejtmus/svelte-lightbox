{
	"name": "svelte-lightbox",
	"version": "1.1.1",
	"license": "MIT",
	"author": {
		"name": "Filip Holčík",
		"email": "filip.holcik.official@gmail.com"
	},
	"repository": "https://github.com/Hejtmus/svelte-lightbox.git",
	"homepage": "https://svelte-lightbox.js.org",
	"description": "Lightweight lightbox library for Svelte",
	"import": "./dist/index.js",
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"publishConfig": {
		"directory": "dist"
	},
	"exports": {
		".": {
			"import": "./dist/index.js",
			"svelte": "./dist/index.js",
			"types": "./dist/index.d.ts"
		}
	},
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"package": "svelte-package",
		"preview": "vite preview",
		"release": "npm run package && npm publish",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "eslint --ignore-path .gitignore ./src"
	},
	"devDependencies": {
		"@sveltejs/adapter-vercel": "^2.0.4",
		"@sveltejs/kit": "1.7.2",
		"@sveltejs/package": "^2.0.1",
		"@sveltejs/site-kit": "^2.1.4",
		"@sveltejs/svelte-repl": "^0.4.1",
		"@types/marked": "^4.0.8",
		"@typescript-eslint/eslint-plugin": "^5.52.0",
		"@typescript-eslint/parser": "^5.52.0",
		"eslint": "^8.34.0",
		"eslint-config-standard": "^17.0.0",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-n": "^15.6.1",
		"eslint-plugin-promise": "^6.1.1",
		"eslint-plugin-svelte3": "^4.0.0",
		"marked": "^4.2.12",
		"svelte": "^3.55.1",
		"svelte-check": "^3.0.3",
		"svelte-preprocess": "^5.0.1",
		"svelte2tsx": "^0.6.1",
		"tslib": "^2.5.0",
		"typescript": "~4.9.4",
		"vite": "^4.1.2"
	},
	"peerDependencies": {
		"svelte": "^3.25.0"
	},
	"keywords": [
		"svelte",
		"svelteKit",
		"lightbox",
		"image",
		"photo",
		"video",
		"gallery",
		"ssr",
		"modal"
	],
	"type": "module"
}
