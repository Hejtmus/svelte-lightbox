{
	"env": {
		"browser": true,
		"es2021": true,
		"node": true
	},
	"extends": [
		"standard"
	],
	"ignorePatterns": ["*.cjs"],
	"overrides": [{
		"files": ["*.svelte"],
		"processor": "svelte3/svelte3"
	}],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": 12,
		"sourceType": "module"
	},
	"plugins": [
		"svelte3",
		"@typescript-eslint"
	],
	"rules": {
		"indent": [
			"off",
			4
		],
		"import/first": "off",
		"no-multiple-empty-lines": "off"
	}
}
